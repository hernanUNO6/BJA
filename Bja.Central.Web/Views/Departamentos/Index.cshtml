@model IEnumerable<Bja.Entidades.Departamento>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Departamentos Resgistrados</h2>

<p>
    @Html.ActionLink("Registrar Departamento", "Create", null, new {@class="btn btn-primary"})
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.IdSesion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaUltimaTransaccion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaRegistro)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Codigo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdSesion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaUltimaTransaccion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaRegistro)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Codigo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
