<Window x:Class="Bja.Registro.frmControlParto"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Control de Parto" ShowInTaskbar="False" Height="200" Width="375" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded">
    <Grid Name="InicioDeSesion">
        <CheckBox x:Name="chkDescartar" Content="Descartar este control" HorizontalAlignment="Left" Margin="16,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblFechaControl" Content="Fecha:" HorizontalAlignment="Left" Margin="26,44,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.778,0.641"/>
        <DatePicker x:Name="dtpFechaControl" HorizontalAlignment="Left" Margin="74,44,0,0" VerticalAlignment="Top" Width="100"/>
        <Label x:Name="lblMedico" Content="Médico:" HorizontalAlignment="Left" Margin="17,69,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.778,0.641"/>
        <ComboBox x:Name="cboMedico"  HorizontalAlignment="Left" Margin="74,73,0,0" VerticalAlignment="Top" Width="263"/>
        <Button x:Name="cmdAceptar" Content="Aceptar" HorizontalAlignment="Left" Margin="170,120,0,0" VerticalAlignment="Top" Width="75" Click="cmdAceptar_Click"/>
        <Button x:Name="cmdCancelar" Content="Cancelar" HorizontalAlignment="Left" Margin="262,120,0,0" VerticalAlignment="Top" Width="75" Click="cmdCancelar_Click"/>
    </Grid>
</Window>
