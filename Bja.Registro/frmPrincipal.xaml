<Window x:Class="Bja.Registro.frmPrincipal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Sistema del Bono Juana Azurduy de Padilla" Height="250" Width="250" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Closed="Window_Closed">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
            <GradientStop Color="#FFFDFCFB" Offset="0"/>
            <GradientStop Color="#FFE0982E" Offset="1"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid Name="Principal" >
        <Button x:Name="cmdCorresponsabilidadesDeMadres" Content="Corresponsabilidades de Madres" HorizontalAlignment="Left" Margin="20,25,0,0" VerticalAlignment="Top" Width="202" Click="cmdCorresponsabilidadesDeMadres_Click"/>
        <Button x:Name="cmdCorresponsabilidadesDeMenores" Content="Corresponsabilidades de Menores" HorizontalAlignment="Left" Margin="20,68,0,0" VerticalAlignment="Top" Width="202" Click="cmdCorresponsabilidadesDeMenores_Click"/>
        <Button x:Name="cmdAdministracion" Content="Administración" HorizontalAlignment="Left" Margin="20,113,0,0" VerticalAlignment="Top" Width="202" Click="cmdAdministracion_Click"/>
        <Button x:Name="cmdSalir" Content="Salir" HorizontalAlignment="Left" Margin="20,164,0,0" VerticalAlignment="Top" Width="202" Click="cmdSalir_Click"/>
    </Grid>
</Window>
