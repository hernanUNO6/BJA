<Window x:Class="Bja.Registro.frmOperacion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Parámetros de operación" ShowInTaskbar="False" Height="200" Width="400" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" Loaded="Window_Loaded">
    <Grid Name="ParametrosOperacion">
        <Label x:Name="lblEstablecimiento" Content="Establecimiento de salud" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblActual" Content="Actual" HorizontalAlignment="Left" Margin="26,36,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cboActual" IsEnabled="False" HorizontalAlignment="Left" Margin="74,41,0,0" VerticalAlignment="Top" Width="294"/>
        <Label x:Name="lblNuevo" Content="Nuevo" HorizontalAlignment="Left" Margin="24,73,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="cboNuevo" HorizontalAlignment="Left" Margin="74,79,0,0" VerticalAlignment="Top" Width="294"/>
        <Button x:Name="cmdAceptar" Content="Aceptar" HorizontalAlignment="Left" Margin="196,122,0,0" VerticalAlignment="Top" Width="75" Click="cmdAceptar_Click"/>
        <Button x:Name="cmdCancelar" Content="Cancelar" HorizontalAlignment="Left" Margin="293,122,0,0" VerticalAlignment="Top" Width="75" Click="cmdCancelar_Click"/>
    </Grid>
</Window>
